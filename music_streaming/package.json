{
  "name": "pozdnako-y3",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "mongod --fork --logpath /var/log/mongodb.log && node --inspect=0.0.0.0 ./bin/www",
    "test": "jest --coverage ",
    "watch": "node_modules/.bin/jest --coverage --watchAll"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": -10
      }
    },
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**"
    ]
  },
  "dependencies": {
    "async": "latest",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "express-handlebars": "^3.0.0",
    "form-data": "^2.3.3",
    "gridfs-stream": "^1.1.1",
    "http-errors": "~1.6.2",
    "http-status-codes": "^1.3.0",
    "mongoose": "^5.3.13",
    "morgan": "~1.9.0",
    "multiparty": "^4.2.1",
    "node-sass-middleware": "0.11.0",
    "sha256": "^0.2.0",
    "validate-fields": "^5.1.1"
  },
  "devDependencies": {
    "rewire": "4.0.1",
    "eslint": "^5.8.0",
    "jest": "^23.6.0",
    "supertest": "3.3.0"
  }
}
