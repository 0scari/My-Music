<div id="page-wrapper">
    <div class="inner-content">
        <div class="music-browse">
            <!--albums-->
            <!-- pop-up-box -->
            <link href="/css/popuo-box.css" rel="stylesheet" type="text/css" media="all">
            <script src="/js/jquery.magnific-popup.js" type="text/javascript"></script>
            <script src="/my-js/playlists_body.js"></script>
            <script>
                $(document).ready(function() {
                    $('.popup-with-zoom-anim').magnificPopup({
                        type: 'inline',
                        fixedContentPos: false,
                        fixedBgPos: true,
                        overflowY: 'auto',
                        closeBtnInside: true,
                        preloader: false,
                        midClick: true,
                        removalDelay: 300,
                        mainClass: 'my-mfp-zoom-in'
                    });
                });
            </script>
            <!--//pop-up-box -->

            <div class="browse">
                <div class="tittle-head two">
                    <div class="col-lg-4 " style="padding: 0">
                            <div class="input-group">
                                <input type="text" name="name" class="form-control" placeholder="New playlist name...">
                                <span class="input-group-btn">
												<button id="newPlaylist" class=" btn btn-primary" type="submit">Add</button>
											  </span>
                            </div><!-- /input-group -->
                    </div><!-- /.col-lg-6 -->
                    <div class="clearfix"> </div>
                </div>

                {{#each playlists}}
                    <div class="col-md-3 browse-grid">
                        <a  href="/playlist/{{ this.dataValues.id }}">
                            <img src="/images/playlist-{{#if this.dataValues.imageData}}{{ this.dataValues.id }}{{ else }}default{{/if}}.png"
                                 title="allbum-name" style="height: 180px; width: 180px">
                        </a>
                        <a><i class="glyphicon glyphicon-play-circle"></i></a>
                        <a class="sing" href="/playlist/{{ this.dataValues.id }}">{{ this.dataValues.name }}</a>
                    </div>
                {{else}}
                    <h4 class="title">No playlists yet...</h4>
                {{/each}}

                <div class="clearfix"> </div>
            </div>
        </div>

